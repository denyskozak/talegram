{
  "name": "reactjs-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://telegram-mini-apps.github.io/open-reader",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "dev:https": "cross-env HTTPS=true vite",
    "dev:admin": "cd admin && npm run dev",
    "build": "tsc --noEmit && vite build",
    "test": "npm run test:build && npm run test:run",
    "test:build": "tsc --project tsconfig.test.json",
    "test:run": "node --test --experimental-specifier-resolution=node dist-test/shared/lib/__tests__/*.test.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@mysten/walrus": "^0.8.3",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf/renderer": "^4.3.1",
    "@telegram-apps/telegram-ui": "^2.1.9",
    "@tma.js/sdk": "^3.1.0",
    "@tma.js/sdk-react": "^3.0.9",
    "@ton/core": "^0.62.0",
    "@ton/ton": "^16.0.0",
    "@tonconnect/ui-react": "^2.3.1",
    "@trpc/client": "^11.7.1",
    "eruda": "^3.4.3",
    "i": "^0.3.7",
    "i18next": "^25.6.0",
    "npm": "^11.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.0.0",
    "react-pdf": "^10.2.0",
    "react-reader": "^2.0.15",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
